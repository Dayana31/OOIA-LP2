<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
   
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="CursoLlevadoWSPortBinding" />
                <binding name="EvaluacionWSPortBinding" />
                <binding name="CursoWSPortBinding" />
                <binding name="AlumnoWSPortBinding" />
                <binding name="CodigoAtencionWSPortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/WebServicesOOIA/CursoLlevadoWS"
                binding="basicHttpBinding" bindingConfiguration="CursoLlevadoWSPortBinding"
                contract="CursoLlevadoWS.CursoLlevadoWS" name="CursoLlevadoWSPort" />
            <endpoint address="http://localhost:8080/WebServicesOOIA/EvaluacionWS"
                binding="basicHttpBinding" bindingConfiguration="EvaluacionWSPortBinding"
                contract="EvaluacionWS.EvaluacionWS" name="EvaluacionWSPort" />
            <endpoint address="http://localhost:8080/WebServicesOOIA/CursoWS"
                binding="basicHttpBinding" bindingConfiguration="CursoWSPortBinding"
                contract="CursoWS.CursoWS" name="CursoWSPort" />
            <endpoint address="http://localhost:8080/WebServicesOOIA/AlumnoWS"
                binding="basicHttpBinding" bindingConfiguration="AlumnoWSPortBinding"
                contract="AlumnoWS.AlumnoWS" name="AlumnoWSPort" />
            <endpoint address="http://localhost:8080/WebServicesOOIA/CodigoWS"
                binding="basicHttpBinding" bindingConfiguration="CodigoAtencionWSPortBinding"
                contract="CodigoWS.CodigoAtencionWS" name="CodigoAtencionWSPort" />
        </client>
    </system.serviceModel>
</configuration>